{
  "name": "olly",
  "version": "0.0.0-dev.7",
  "description": "Micro framework for building REST API",
  "main": "lib/service",
  "scripts": {
    "test": "NODE_ENV=test mocha -r should -R spec",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- -r should -R spec",
    "build": "gulp build",
    "dev": "nodemon -w ./src --exec gulp build",
    "prepublish": "npm run test && npm run build"
  },
  "bin": {
    "olly": "bin/olly.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtomixInteractions/olly.git"
  },
  "keywords": [
    "server",
    "raml",
    "http",
    "api",
    "rest",
    "express"
  ],
  "author": "Sergey Sova <i.am@lestad.net> (https://lestad.net)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/AtomixInteractions/olly/issues"
  },
  "homepage": "https://github.com/AtomixInteractions/olly#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^5.8.34",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-rimraf": "^0.2.0",
    "mocha": "^2.3.4",
    "pegjs": "^0.9.0",
    "should": "^8.1.1",
    "webpack": "^1.12.9"
  }
}
