{
  "name": "olly",
  "version": "0.0.2",
  "description": "Micro framework for building REST API",
  "main": "lib/index",
  "scripts": {
    "test": "NODE_ENV=test mocha -r should -R spec",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- -r should -R spec",
    "build": "gulp build",
    "dev": "nodemon -w ./src --exec gulp build",
    "prepublish": "npm run build && npm run test",
    "demo": "DEBUG=olly,olly:application,olly:generate nodemon ./demo.js",
    "changelog": "changelog-maker --group true --filter-release true"
  },
  "bin": {
    "olly": "bin/olly.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtomixInteractions/olly.git"
  },
  "keywords": [
    "server",
    "raml",
    "http",
    "api",
    "rest",
    "express"
  ],
  "author": "Sergey Sova <i.am@lestad.net> (https://lestad.net)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/AtomixInteractions/olly/issues"
  },
  "contributors": [
    "Ivan Takarlikov <vtakarlikov@gmail.com> (http://takarlikov.com)"
  ],
  "homepage": "https://github.com/AtomixInteractions/olly#readme",
  "dependencies": {
    "express": "^4.13.4",
    "nonstandard": "^0.10.3-toc",
    "olly.parser": "^0.0.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^5.8.34",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "debug": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-rimraf": "^0.2.0",
    "mocha": "^2.4.5",
    "should": "^8.2.1"
  }
}
